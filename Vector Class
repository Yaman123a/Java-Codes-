import java.util.Vector;

public class VectorEx {
    public static void main(String[] args) {
        Vector<Integer> vector = new Vector<Integer>();
        //Adding elements to vector
        vector.add(55);
        vector.add(15);
        vector.add(22);
        vector.add(45);
        vector.add(16);

        //Inserting new element at 3rd position
        vector.add(2, 33);

        //Removing 1st and 4th elements from vector
        vector.remove(0);
        vector.remove(2);

        //Printing the final vector
        System.out.println("Vectors elements after modifications:");
        for(int i= 0; i < vector.size(); i++) System.out.print(vector.get(i) + " ");
    }
}
